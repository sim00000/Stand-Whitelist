local commands = {}
local prefix = "/"

commands.e = function(arguments)
	local CMD = arguments[1]
	if CMD == 'glitch' then
		if arguments[2] then
			local target1 = tostring(findPlayer(arguments[2]))
			follow2 = false
			noclip2 = false
			bring = false 
            
	resetplayer2 = Game.Players.LocalPlayer
	resetplayer2.Character.HumanoidRootPart.CFrame = CFrame.new(-591.085022, 8.31477833, -742.803528, 0.994070172, 5.60480942e-08, 0.108740292, -6.51724079e-08, 1, 8.03552425e-08, -0.108740292, -8.69656134e-08, 0.994070172)
	
	game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(game:GetService("Players")[target1].Character.UpperTorso.Position) * CFrame.new(0,1,0)
	game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(game:GetService("Players")[target1].Character.UpperTorso.Position) * CFrame.new(0,1,0)
	game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(game:GetService("Players")[target1].Character.UpperTorso.Position) * CFrame.new(0,1,0)
	wait(0.4)
	local args = {
    [1] = "Grabbing",
    [2] = false
}

game:GetService("ReplicatedStorage").MainEvent:FireServer(unpack(args))

game:GetService("Workspace").Players[alt2].BodyEffects.Grabbed:Destroy()
	
	wait(60)
	resetplayer.Character.Head:Destroy()
		resetplayer2.Character.RightHand:Destroy()
		resetplayer2.Character.LeftHand:Destroy()
		resetplayer2.Character.LowerTorso:Destroy()
		resetplayer2.Character.UpperTorso:Destroy()
		resetplayer2.Character.LeftLowerLeg:Destroy()
		resetplayer2.Character.RightLowerLeg:Destroy()
		resetplayer2.Character.RightFoot:Destroy()
		resetplayer2.Character.LeftFoot:Destroy()
		resetplayer2.Character.LeftUpperLeg:Destroy()
		resetplayer2.Character.RightUpperLeg:Destroy()
		resetplayer2.Character.RightUpperArm:Destroy()
		resetplayer2.Character.RightLowerArm:Destroy()
		resetplayer2.Character.LeftLowerArm:Destroy()
		resetplayer2.Character.LeftUpperArm:Destroy()
	

	



		end
	end
end


game.Players[OWNER].Chatted:Connect(function(msg)
msg = string.lower(msg)
local splitString = msg:split(" ")
local slashCommand = splitString[1] 
local cmd = slashCommand:split(prefix)    
local cmdName = cmd[2]    
if commands[cmdName] then 
local arguments = {} 
for i = 2, #splitString, 1 do
table.insert(arguments,splitString[i])            
end
commands[cmdName](arguments)
end
end)
else
	print("declined")
end
end

