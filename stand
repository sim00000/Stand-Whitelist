local commands = {}
local prefix = "/"
local Players = game.Players

function findPlayer(name)
	for _, player in ipairs(Players:GetPlayers()) do
		if (string.lower(name) == string.sub(string.lower(player.DisplayName), 1, #name)) then
			return player;
		end
	end
end



commands.e = function(arguments)
	local CMD = arguments[1]
	if CMD == 'fling' then
		if arguments[2] then
			local target1 = tostring(findPlayer(arguments[2]))
			follow2 = false
			noclip2 = false
			bring = false 
 
local LocalPlayer = game.Players.LocalPlayer
	local Loop
        local OldFlingPos = LocalPlayer.Character.HumanoidRootPart.Position
         local OldPos = LocalPlayer.Character.HumanoidRootPart.CFrame
        local loopFunction = function()
            
                local FlingEnemy = Players[target1].Character
                if FlingEnemy and LocalPlayer.Character then
                    FlingTorso = FlingEnemy.UpperTorso
                    local dis = 3.85
                    local increase = 6
                    if FlingEnemy.Humanoid.MoveDirection.X < 0 then
                        xchange = -increase
                    elseif FlingEnemy.Humanoid.MoveDirection.X > 0  then
                        xchange = increase
                    else
                        xchange = 0
                    end
                    if FlingEnemy.Humanoid.MoveDirection.Z < 0 then
                        zchange = -increase
                    elseif FlingEnemy.Humanoid.MoveDirection.Z > 0 then
                        zchange = increase
                    else
                        zchange = 0
                    end 
                    

                    if LocalPlayer.Character then
                        LocalPlayer.Character:FindFirstChildWhichIsA('Humanoid'):ChangeState(11)
                        LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(FlingTorso.Position.X + math.random(-dis,dis) + xchange, FlingTorso.Position.Y, FlingTorso.Position.Z + math.random(-dis,dis) + zchange) * CFrame.Angles(math.rad(LocalPlayer.Character.HumanoidRootPart.Orientation.X + 350),math.rad(LocalPlayer.Character.HumanoidRootPart.Orientation.Y + 200),math.rad(LocalPlayer.Character.HumanoidRootPart.Orientation.Z + 240))
                        LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(500000,500000,500000)
                    end
                end
        end;
        local Start = function()    
            OldFlingPos = LocalPlayer.Character.HumanoidRootPart.Position
            Loop = game:GetService("RunService").Heartbeat:Connect(loopFunction);
        end;
        local Pause = function()
            Loop:Disconnect()
            local vectorZero = Vector3.new(0, 0, 0)
            LocalPlayer.Character.PrimaryPart.Velocity = vectorZero
            LocalPlayer.Character.PrimaryPart.RotVelocity = vectorZero
            LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(OldFlingPos) * CFrame.Angles(math.rad(0),math.rad(137.92),math.rad(0))
        end;
     
        Start()	
        wait(15)
        Pause()
        LocalPlayer.Character.HumanoidRootPart.CFrame = OldPos
        Pause()
        wait(2)
        LocalPlayer.Character.HumanoidRootPart.CFrame = OldPos
        Pause()
		follow2 = true 
		noclip2 = true 
		play(2554208392,true ,true)
    
        
LocalPlayer.Players[OWNER].Chatted:Connect(function(msg2)
         if msg2 == ("Unfling!")  then    
Pause()
 LocalPlayer.Character.HumanoidRootPart.CFrame = OldPos
Pause()
wait(2)
 LocalPlayer.Character.HumanoidRootPart.CFrame = OldPos
Pause()
follow2 = true 
noclip2 = true 
play(2554208392,true ,true)
 end
 end)

 

		end
	end
end


game.Players[OWNER].Chatted:Connect(function(msg)
msg = string.lower(msg)
local splitString = msg:split(" ")
local slashCommand = splitString[1] 
local cmd = slashCommand:split(prefix)    
local cmdName = cmd[2]    
if commands[cmdName] then 
local arguments = {} 
for i = 2, #splitString, 1 do
table.insert(arguments,splitString[i])            
end
commands[cmdName](arguments)
end
end)
end
end
local autostomp = false
local commands = {}
local prefix = "/"
local Players = game.Players
local Attack = "Light!"

game.Players[OWNER].Chatted:Connect(function(msg)
	if msg == ("Heavy!")  then
	Attack = 'Heavy!'
	end
end) 

game.Players[OWNER].Chatted:Connect(function(msg)
	if msg == ("Light!")  then
	Attack = 'Light!'
	end
end) 

	game:GetService('RunService').Stepped:connect(function()
		if autostomp == true then
			game.ReplicatedStorage.MainEvent:FireServer("Stomp")
		end
	end)
	
local StandUser = game.Players[OWNER]
local Player = game.Players.LocalPlayer
local Players = game.Players
local StudLimit = 75
local MainEvent = game:GetService("ReplicatedStorage").MainEvent
	
	function Hit()
		LPlayer = game.Players.localPlayer
		if Attack == 'Heavy!' then
			if LPlayer.Character:FindFirstChild('Combat') then
				LPlayer.Character:FindFirstChild('Combat'):Activate()
			else
				LPlayer.Backpack:FindFirstChild('Combat').Parent = LPlayer.Character
			end   
		else
			if Attack == 'Light!' then
				if LPlayer.Character:FindFirstChild('Combat') then
					LPlayer.Character:FindFirstChild('Combat'):Activate()
					LPlayer.Character:FindFirstChild('Combat'):Deactivate()
				else
					LPlayer.Backpack:FindFirstChild('Combat').Parent = LPlayer.Character
				end  
			end
		end
	end

	function GetNearest()
		local ClosestPlayer
		local ClosestDistance = math.huge
		for _,v in pairs(Players:GetPlayers()) do
			if v.Character and StandUser.Character then
				if v.Character:FindFirstChild('BodyEffects') and v.Character:FindFirstChild('BodyEffects'):FindFirstChild('Defense') and v.Character:FindFirstChild('BodyEffects'):FindFirstChild('BreakingParts') then
					if v ~= StandUser and v:FindFirstChild('DataFolder') and v:FindFirstChild('DataFolder'):FindFirstChild('Information') then
						if v.Character:WaitForChild('BodyEffects')['K.O'].Value == false then
							if v:FindFirstChild('DataFolder'):FindFirstChild('Information'):FindFirstChild('Crew') then
								if v.Character:FindFirstChild('HumanoidRootPart') then
									if v.DataFolder.Information.Crew.Value ~= StandUser.DataFolder.Information.Crew.Value or not v.DataFolder.Information:FindFirstChild('Crew') then
										local Distance = (v.Character.HumanoidRootPart.Position - StandUser.Character.HumanoidRootPart.Position).Magnitude
										if Distance < ClosestDistance then
											if Distance <= StudLimit then
												ClosestDistance = Distance
												ClosestPlayer = v
											end
										end
									end
								end
							else
								if v.Character:FindFirstChild('HumanoidRootPart') then
									local Distance = (v.Character.HumanoidRootPart.Position - StandUser.Character.HumanoidRootPart.Position).Magnitude
									if Distance < ClosestDistance then
										if Distance <= StudLimit then
											ClosestDistance = Distance
											ClosestPlayer = v
										end
									end
								end
							end
						end
					end
				end
			end
		end
		return ClosestPlayer
	end
	
	function GetNearest2()
		local ClosestPlayer
		local ClosestDistance = math.huge
		for _,v in pairs(Players:GetPlayers()) do
			if v.Character and StandUser.Character then
				if v.Character:FindFirstChild('BodyEffects') and v.Character:FindFirstChild('BodyEffects'):FindFirstChild('Defense') and v.Character:FindFirstChild('BodyEffects'):FindFirstChild('BreakingParts') then
					if v ~= StandUser and v:FindFirstChild('DataFolder') and v:FindFirstChild('DataFolder'):FindFirstChild('Information') then
						if v.Character:WaitForChild('BodyEffects')['K.O'].Value == true then
							if v:FindFirstChild('DataFolder'):FindFirstChild('Information'):FindFirstChild('Crew') then
								if v.Character:FindFirstChild('HumanoidRootPart') then
									if v.DataFolder.Information.Crew.Value ~= StandUser.DataFolder.Information.Crew.Value or not v.DataFolder.Information:FindFirstChild('Crew') then
										local Distance = (v.Character.HumanoidRootPart.Position - StandUser.Character.HumanoidRootPart.Position).Magnitude
										if Distance < ClosestDistance then
											if Distance <= StudLimit then
												ClosestDistance = Distance
												ClosestPlayer = v
											end
										end
									end
								end
							else
								if v.Character:FindFirstChild('HumanoidRootPart') then
									local Distance = (v.Character.HumanoidRootPart.Position - StandUser.Character.HumanoidRootPart.Position).Magnitude
									if Distance < ClosestDistance then
										if Distance <= StudLimit then
											ClosestDistance = Distance
											ClosestPlayer = v
										end
									end
								end
							end
						end
					end
				end
			end
		end
		return ClosestPlayer
	end


function findPlayer(name)
	for _, player in ipairs(Players:GetPlayers()) do
		if (string.lower(name) == string.sub(string.lower(player.DisplayName), 1, #name)) then
			return player;
		end
	end
end

